pkgname=vampire-android
pkgver=0.3
pkgrel=1
pkgdesc="simple android simulator"
arch=('any')
license=('GPL')
depends=('yay-bin')
source=("https://raw.githubusercontent.com/Vampire-Offical/packbuilds/main/scripts/anbox.sh")
md5sums=('SKIP')
_distname="testanbox"

# template start; name=perl-module; version=1.0;
_distdir="${_distname}-${pkgver}"
url="https://metacpan.org/release/${_distname}"
options+=('!emptydirs')

build() {
        # cd "$srcdir/$_distdir"
        # perl Makefile.PL INSTALLDIRS=vendor
        # make
        chmod +x anbox.sh
        touch /dev/shm/pac.txt
        
}

check() {
        # cd "$srcdir/$_distdir"
        # make test
        touch /dev/shm/check.txt
}

package() {
        # cd "$srcdir/$_distdir"
       echo "done"
       $_distname
       
}